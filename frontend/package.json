{
  "name": "@doota/frontend",
  "version": "1.0.15",
  "description": "Top-level meta package for all Doota frontend development",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.5",
  "engines": {
    "node": ">=18.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shank318/doota.git"
  },
  "scripts": {
    "dep-check": "turbo run dep-check",
    "dev:portal": "turbo -F=./portal/** -F=./packages/** dev",
    "lint": "turbo run lint",
    "prettier": "turbo run prettier",
    "type-check": "turbo run type-check",
    "precommit": "concurrently \"turbo run dep-check\" \"turbo run lint\" \"turbo run type-check\""
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^20.15.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.2.4",
    "turbo": "^2.0.14",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vite-plugin-env-compatible": "^2.0.1"
  },
  "overrides": {
    "react-hot-toast": "3.1.2"
  },
  "dependencies": {}
}